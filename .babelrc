{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "actions": "./src/action",
        "app": "./app.json",
        "components": "./src/components",
        "reducers": "./src/reducers",
        "styles": "./src/styles"
      }
    }]
  ]
}